{
  "name": "git-auto-deploy",
  "version": "0.1.1",
  "description": "deploy project automatically from your github repo",
  "main": "server.js",
  "scripts": {
    "build": "gulp build",
    "watch": "browserify app/react/main.js -o public/js/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "browserify": "browserify app/react/main.js -o public/js/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "start": "gulp start --silent",
    "start:prod": "npm run build & node server.js",
    "lint": "./node_modules/.bin/eslint . --ext .jsx,.js",
    "test": "mocha \"test/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NikaBuligini/git-auto-deploy"
  },
  "author": "nb",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "events": "^1.1.1",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.14.0",
    "github": "^2.4.0",
    "humps": "^1.1.0",
    "jquery": "^3.1.0",
    "mongoose": "^4.5.5",
    "node-jsx": "^0.13.3",
    "normalizr": "^2.2.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-redux-loading-bar": "^2.3.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-tabs": "^0.7.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "socket.io": "^1.4.8",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "debug": "^2.2.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0-beta.3",
    "eslint-plugin-mocha": "^4.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "expect": "^1.20.2",
    "font-awesome": "^4.6.3",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.4.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-nodemon": "^2.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "mocha": "^2.5.3",
    "node-notifier": "^4.6.0",
    "react-addons-test-utils": "^15.2.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "uglify-js": "^2.7.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^5.0.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
