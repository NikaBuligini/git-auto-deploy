{
  "name": "git-auto-deploy",
  "version": "0.1.0",
  "description": "deploy project automatically from your github repo",
  "main": "server.js",
  "scripts": {
    "build": "gulp build",
    "watch": "watchify app.js -o public/js/bundle.js -v",
    "browserify": "browserify app.js | uglifyjs > public/js/bundle.js",
    "start": "gulp start --silent",
    "serve": "npm run build & node server.js",
    "test": "mocha \"test/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NikaBuligini/git-auto-deploy"
  },
  "author": "nb",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "events": "^1.1.1",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.14.0",
    "github": "^2.4.0",
    "mongoose": "^4.5.5",
    "node-jsx": "^0.13.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "request": "^2.74.0",
    "socket.io": "^1.4.8",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.2.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "debug": "^2.2.0",
    "expect": "^1.20.2",
    "font-awesome": "^4.6.3",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-nodemon": "^2.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^2.0.0",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.2.1",
    "reactify": "^1.1.1",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
