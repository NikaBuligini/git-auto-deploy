{
  "name": "git-auto-deploy",
  "version": "0.0.1",
  "description": "deploy project automatically from your github repo",
  "main": "server.js",
  "scripts": {
    "start": "npm run watch & node server.js",
    "watch": "watchify app.js -o public/js/bundle.js -v",
    "browserify": "browserify app.js | uglifyjs > public/js/bundle.js",
    "build": "npm run browserify ",
    "test": "mocha \"test/*.spec.js\""
  },
  "author": "nb",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.14.0",
    "mongoose": "^4.5.5",
    "node-jsx": "^0.13.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "expect": "^1.20.2",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.2.1",
    "react-object-inspector": "^0.2.1",
    "reactify": "^1.1.1",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
